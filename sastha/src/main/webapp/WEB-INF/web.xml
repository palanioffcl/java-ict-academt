<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>sastha</display-name>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>ClientLogin</display-name>
    <servlet-name>ClientLogin</servlet-name>
    <servlet-class>login.ClientLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ClientLogin</servlet-name>
    <url-pattern>/ClientLogin</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>HelloWorld</display-name>
    <servlet-name>HelloWorld</servlet-name>
    <servlet-class>welcome.HelloWorld</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloWorld</servlet-name>
    <url-pattern>/HelloWorld</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>Logger</display-name>
    <filter-name>Logger</filter-name>
    <filter-class>welcome.Logger</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Logger</filter-name>
    <url-pattern>/HelloWorld</url-pattern>
  </filter-mapping>
</web-app>